{
  "main": "index.js",
  "scripts": {
    "hex": "ts-node --project src/server/tsconfig.json src/server/commands",
    "analyse-size": "DATE=$(date '+%Y%m%d-%H%M%S'); NODE_ENV=production webpack --profile --json | grep -E '^{' | sed -E 's/Done in .+//' > size-stats-$DATE.json && webpack-bundle-analyzer size-stats-$DATE.json dist/statics/",
    "lint": "eslint src --ext .ts,.vue && vue-tsc -p src/client --noEmit",
    "start": "node index.js",
    "start:prod": "NODE_ENV=production node index.js",
    "type-check-client": "tsc -p ./src/client/tsconfig.json",
    "build-client": "NODE_ENV=production TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack --mode production",
    "build-server": "tsc -b src/server",
    "dev-client": "TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack serve",
    "dev-server": "tsc-watch --noClear -b ./src/server/tsconfig.json --onSuccess \"node --enable-source-maps index.js\"",
    "serve": "concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgBlue.bold,bgGreen.bold\" \"yarn dev-server\" \"yarn dev-client\"",
    "serve:prod": "yarn build && yarn start:prod",
    "build": "concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgBlue.bold,bgGreen.bold\" \"yarn build-server\" \"yarn build-client\"",
    "db:diff": "yarn build-server && typeorm schema:log -d dist/server/data-source.js",
    "db:sync": "yarn build-server && typeorm schema:sync -d dist/server/data-source.js",
    "test:unit": "TS_NODE_PROJECT='src/shared/tsconfig.json' mocha src/**/**/test/**/*.ts -n import=ts-node/esm --extension ts",
    "test:e2e": "cypress run",
    "test:all": "yarn test:unit && yarn test:e2e",
    "test": "yarn test:unit"
  },
  "license": "AGPL-3.0",
  "dependencies": {

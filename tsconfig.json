{
    "compilerOptions": {
        "module": "NodeNext",
        "moduleResolution": "nodenext",
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noImplicitReturns": true,
        "noImplicitOverride": true,
        "resolveJsonModule": true,
        "emitDecoratorMetadata": true,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "strictNullChecks": true
    }
}
